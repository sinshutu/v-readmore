{
  "name": "vue-readmore",
  "version": "1.0.0",
  "description": "A Vue.js read more component",
  "main": "src/index.js",
  "repository": "https://github.com/sinshutu/vue-readmore",
  "author": "sinshutu",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack -w --mode=development --config webpack.config.js --progress --hide-modules",
    "build": "webpack --mode=production --config webpack.config.js --progress --hide-modules"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^5.1.0",
    "eslint-plugin-html": "^4.0.5",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
